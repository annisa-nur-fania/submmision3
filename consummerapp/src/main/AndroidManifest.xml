<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dicoding.picodiploma.consummerapp">

    <permission
        android:name="com.dicoding.picodiploma.submission2annisa.READ_DATABASE"/>
    <permission
        android:name="com.dicoding.picodiploma.submission2annisa.WRITE_DATABASE"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Submission2Annisa">
        <activity android:name=".Favorite.AvatarFavActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Favorite.FavDetailActivity"/>
    </application>
    <queries>
        <package android:name="com.dicoding.picodiploma.submission2annisa"/>
    </queries>

</manifest>